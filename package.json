{
  "name": "@alshdavid/kit",
  "version": "0.8.6",
  "license": "MIT",
  "exports": {
    "./*": {
      "types": "./release/types/*/index.d.ts",
      "import": "./release/esm/*/index.js",
      "require": "./release/require/*/index.js"
    }
  },
  "scripts": {
    "prettier": "npx prettier -w .",
    "build": "npx ts-node ./.github/scripts/cmd/build/main.ts",
    "clean": "rm -rf release",
    "serve": "npx http-server -c=-1 --cors=true ./release/umd",
    "watch": "npx nodemon --watch \"./lib/**/*\" --watch \"./platform/**/*\" --signal SIGTERM --ext \"*\" --exec \"node ./.github/scripts/cmd/build.mjs || exit 1\""
  },
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.354.0",
    "@aws-sdk/client-s3": "^3.354.0",
    "@types/mime-types": "^2.1.1",
    "mime-types": "^2.1.35",
    "yaml": "^2.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "@types/semver": "^7.5.0",
    "http-server": "^14.1.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "semver": "^7.5.2",
    "swc-loader": "^0.2.3",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "webpack": "^5.87.0"
  },
  "files": [
    "./release",
    "./package.json",
    "./LICENSE",
    "README.md"
  ]
}
