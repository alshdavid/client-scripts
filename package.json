{
  "name": "@alshdavid/kit",
  "version": "0.0.3",
  "license": "MIT",
  "exports": {
    "./*": {
      "types": "./release/types/*/lib/index.d.ts",
      "import": "./release/esm/*/lib/index.js",
      "require": "./release/require/*/lib/index.js"
    },
    "./lib/*": {
      "types": "./release/types/*/lib/index.d.ts",
      "import": "./release/esm/*/lib/index.js",
      "require": "./release/require/*/lib/index.js"
    },
    "./platform/*": {
      "types": "./release/types/*/platform/index.d.ts",
      "import": "./release/esm/*/platform/index.js",
      "require": "./release/require/*/platform/index.js"
    }
  },
  "scripts": {
    "prettier": "npx prettier -w .",
    "build": "npx ts-node ./.github/scripts/cmd/build/main.ts",
    "clean": "rm -rf release",
    "serve": "npx http-server -c=-1 --cors=true ./release/umd",
    "watch": "npx nodemon --watch \"./lib/**/*\" --watch \"./platform/**/*\" --signal SIGTERM --ext \"*\" --exec \"node ./.github/scripts/cmd/build.mjs || exit 1\""
  },
  "devDependencies": {
    "@aws-sdk/client-cloudfront": "^3.332.0",
    "@aws-sdk/client-s3": "^3.332.0",
    "@rspack/core": "^0.1.10",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^20.1.4",
    "http-server": "^14.1.1",
    "mime-types": "^2.1.35",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "swc-loader": "^0.2.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "webpack": "^5.82.1"
  },
  "files": [
    "./release",
    "./package.json",
    "./LICENSE"
  ]
}
